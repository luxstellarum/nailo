						a.province(id="kyunggido",href="/plan/plan#plan_2", rel='external',data-effect='slide', style='position:absolute; width:50px; height: 50px; top:30%; left:27%; z-index:2;') 경기도
						a.province(id="chungcheongdo",href="/plan/plan#plan_2", rel='external',data-effect='slide', style='position:absolute; width:50px; height: 50px; top:50%; left:35%; z-index:2;') 충청도
						a.province(id="kyungsangdo",href="/plan/plan#plan_2", rel='external',data-effect='slide', style='position:absolute; width:50px; height: 50px; top:60%; left:64%; z-index:2;') 경상도
						a.province(id="jeollado",href="/plan/plan#plan_2", rel='external',data-effect='slide', style='position:absolute; width:50px; height: 50px; top:70%; left:25%; z-index:2;') 전라도
					
					
				div(id="plan_2",class="slide")
