						font.province(id="chungcheongdo", style='position:absolute; width:50px; height: 50px; top:50%; left:35%; z-index:2; display:none') 충청도
						font.province(id="kyungsangdo", style='position:absolute; width:50px; height: 50px; top:60%; left:64%; z-index:2; display:none') 경상도
						font.province(id="jeollado", style='position:absolute; width:50px; height: 50px; top:70%; left:25%; z-index:2; display:none') 전라도
					div(align="right",style="margin-top:-30px; padding-right:10px")	
						span
							input(type="button",value="담기", name="insert",style="width:60px; height:30px; margin-right:5px")
							input(type="button",value="확정",name="confirm",style="width:60px; height:30px")
			include ../bottom.jade