		input.btn_save(type="button", value="SAVE", name="save")
	img.map(src="/images/map.png", style='position:absolute; height:300px; top:10%; left:10%; z-index:1;')
	script(src="http://code.jquery.com/jquery-1.4.2.min.js", type="text/javascript")
