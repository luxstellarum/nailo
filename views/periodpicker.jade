div#periodpicker
		ul(style="margin-top:0px; padding-left:0px")
			li 날짜를 선택하세요!
			li(style="margin-left: 7%")
				label(style="border-radius:3px; background-color:#4B4B4B; color:#BCF1ED") 출발
				input.period.start_year(type="text") 
				label.period_label 년 
				input.period.start_month(type="text") 
				label.period_label 월 
				input.period.start_day(type="text") 
				label.period_label 일 
			li(style="margin-left: 7%")
				label 도착
				input.period.end_year(type="text") 
				label.period_label 년 
				input.period.end_month(type="text") 
				label.period_label 월 
				input.period.end_day(type="text") 
				label.period_label 일 
			li(style="text-align:center;")
				input.btn_set_period(type="button", value="입력")
				input.btn_cancel_period(type="button", value="취소")